    var context = {};
    var headers = [];
    var objHalfyearHeader = {};

    objHalfyearHeader["halfyearHeader"] = 'ลำดับ';
    headers.push(objHalfyearHeader);
    
    objHalfyearHeader = {};
    objHalfyearHeader["halfyearHeader"] = 'รถ';
    headers.push(objHalfyearHeader);

    objHalfyearHeader = {};
    objHalfyearHeader["halfyearHeader"] = 'ทะเบียนรถ';
    headers.push(objHalfyearHeader);

    // console.log(headers);



    $.each( arrQuater, function( i, val ) {
        objHalfyearHeader = {};
        objHalfyearHeader["halfyearHeader"] = val;
        headers.push(objHalfyearHeader);

        if (i == arrQuater.length -1) {
            objHalfyearHeader = {};
            objHalfyearHeader["halfyearHeader"] = 'Average';
            headers.push(objHalfyearHeader);
        }
    });

    // objHalfyearHeader["halfyearHeader"] = 'Average';
    // headers.push(objHalfyearHeader);

    context["headers"] = headers;

    return context;
  
},