                    var count = 0;
                    
                    if (typeof obj == "object") {
                    
                        if (Object.keys) {
                            count = Object.keys(obj).length;
                        } else if (window._) {
                            count = _.keys(obj).length;
                        } else if (window.$) {
                            count = $.map(obj, function() { return 1; }).length;
                        } else {
                            for (var key in obj) if (obj.hasOwnProperty(key)) count++;
                        }
                        
                    }
                    
                    return count;
                };

