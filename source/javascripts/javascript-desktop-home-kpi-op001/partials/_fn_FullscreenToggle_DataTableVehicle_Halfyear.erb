FullscreenToggle_DataTableVehicle_Halfyear: function () {
	// console.log("FullscreenToggle_DataTableVehicle_Halfyear");


    $('body').on('click', '#btn-fullscreen-datatable-halfyear', function () {
        
        if ($.fn.DataTable.isDataTable('#datatableVehicle_HalfYear') ) { 
            datatableVehicle_HalfYear.columns.adjust().draw();
        }

        if($(this).attr('data-click-state') == 1) {
            $(this).attr('data-click-state', 0);
            // console.log("btn-fullscreen-datatable-weekly, state-00, exit fullscreen");

        } else {
            $(this).attr('data-click-state', 1)
            // console.log("btn-fullscreen-datatable-weekly, state-01, goto fullscreen");
        }
     
    });

  
},

