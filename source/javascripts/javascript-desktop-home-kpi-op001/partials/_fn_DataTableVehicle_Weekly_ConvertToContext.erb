    var context = {};
    var headers = [];
    var objDateHeader = {};

    objDateHeader["dateHeader"] = 'ลำดับ';
    headers.push(objDateHeader);
    
    objDateHeader = {};
    objDateHeader["dateHeader"] = 'รถ';
    headers.push(objDateHeader);

    objDateHeader = {};
    objDateHeader["dateHeader"] = 'ทะเบียนรถ';
    headers.push(objDateHeader);

    // console.log(headers);



    $.each( arrWeeks, function( i, val ) {
        objDateHeader = {};
        objDateHeader["dateHeader"] = val;
        headers.push(objDateHeader);

        if (i == arrWeeks.length -1) {
            objDateHeader = {};
            objDateHeader["dateHeader"] = 'Average';
            headers.push(objDateHeader);
        }
    });

    // objDateHeader["dateHeader"] = 'Average';
    // headers.push(objDateHeader);

    context["headers"] = headers;

    return context;
  
