FullscreenToggle_DataTableVehicleLeader_Monthly: function () {
	// console.log("FullscreenToggle_DataTableVehicleLeader_Monthly");

    $('body').on('click', '#btn-fullscreen-datatable-monthly', function () {
        
        if ($.fn.DataTable.isDataTable('#datatableVehicleLeader_Monthly') ) {
            datatableVehicleLeader_Monthly.columns.adjust().draw();
        }

        if($(this).attr('data-click-state') == 1) {
            $(this).attr('data-click-state', 0);
            // console.log("btn-fullscreen-datatable-daily, state-00, exit fullscreen");

        } else {
            $(this).attr('data-click-state', 1)
            // console.log("btn-fullscreen-datatable-daily, state-01, goto fullscreen");
        }
     
    });

  
},