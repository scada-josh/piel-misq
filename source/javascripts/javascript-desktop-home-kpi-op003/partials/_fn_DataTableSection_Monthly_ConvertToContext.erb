    var context = {};
    var headers = [];
    var objMonthlyHeader = {};

    objMonthlyHeader["monthHeader"] = 'ลำดับ';
    headers.push(objMonthlyHeader);
    

    objMonthlyHeader = {};
    objMonthlyHeader["monthHeader"] = 'ศูนย์';
    headers.push(objMonthlyHeader);

    // console.log(headers);



    $.each( arrMonths, function( i, val ) {
        objMonthlyHeader = {};
        objMonthlyHeader["monthHeader"] = val;
        headers.push(objMonthlyHeader);

        if (i == arrMonths.length -1) {
            objMonthlyHeader = {};
            objMonthlyHeader["monthHeader"] = 'Average';
            headers.push(objMonthlyHeader);
        }
    });

    // objMonthlyHeader["monthHeader"] = 'Average';
    // headers.push(objMonthlyHeader);

    context["headers"] = headers;

    return context;
  
