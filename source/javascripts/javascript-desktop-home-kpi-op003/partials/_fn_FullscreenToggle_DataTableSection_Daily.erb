FullscreenToggle_DataTableSection_Daily: function () {
	// console.log("FullscreenToggle_DataTableSection_Daily");

    $('body').on('click', '#btn-fullscreen-datatable-daily', function () {
        
        if ($.fn.DataTable.isDataTable('#datatableSection_Daily') ) {
            datatableSection_Daily.columns.adjust().draw();
        }

        if($(this).attr('data-click-state') == 1) {
            $(this).attr('data-click-state', 0);
            // console.log("btn-fullscreen-datatable-daily, state-00, exit fullscreen");

        } else {
            $(this).attr('data-click-state', 1)
            // console.log("btn-fullscreen-datatable-daily, state-01, goto fullscreen");
        }
     
    });

  
},